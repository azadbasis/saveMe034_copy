    <string name="animated_rainbow_span2">Emergency Text: </string>
    <string name="animated_rainbow_span3">Phone Book: </string>
    <string name="animated_rainbow_span4">News Feed: </string>
    <string name="animated_rainbow_span5">SMS Tracking: </string>
    <string name="animated_rainbow_span6">FnF Tracking: </string>
